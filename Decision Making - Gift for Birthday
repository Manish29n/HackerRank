#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    if(n%400==0||(n%4==0&&n%100!=0))
        printf("%d is a leap year",n);
    else
        printf("%d is not a leap year",n);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
