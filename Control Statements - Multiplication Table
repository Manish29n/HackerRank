#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,m,i;
    scanf("%d%d",&n,&m);
    printf("Enter n\nEnter m\nThe multiplication table of %d is\n",n);
    for(i=1;i<=m;i++){
        printf("%d*%d=%d\n",i,n,i*n);
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
