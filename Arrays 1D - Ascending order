#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    int n,t;
    cin>>n;
    vector<int> arr;
    for(int i=0;i<n;i++){
        cin>>t;
        arr.push_back(t);
    }
    sort(arr.begin(), arr.end());
    
    cout << "The Sorted array is:"<<endl;
    for (int num : arr) {
        cout << num << endl;
    }

    return 0;
}
