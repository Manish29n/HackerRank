#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,i,j,c=0;
    scanf("%d",&n);
    for(i=0;i<3;i++){
        for(j=0;j<3;j++){
            c++;
            if(c==n){
                printf("%d %d",i,j);
                break;
            }
        }
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
