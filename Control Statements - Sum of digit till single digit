#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,s;
    scanf("%d",&n);
    while(n>=10){
        s=0;
        while(n){
            s+=n%10;
            n/=10;
        }
        n=s;
    }
    printf("%d",n);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
