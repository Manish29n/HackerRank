#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,k=2,s=2;
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        if(i==0)
            printf("0 ");
        else if(i==1)
            printf("2 ");
        else{
            if(i%2==0)
                k+=4;
            s+=k;
            printf("%d ",s);
        }
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
