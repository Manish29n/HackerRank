#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char s[100];
    int n;
    float f;
    char c;
    scanf("%s%d%f %c",s,&n,&f,&c);
    float r = floor(f * 100) / 100;
    printf("Name: %s\nAge: %d\nCGPA: %.2f\nGrade: %c",s,n,r,c);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
